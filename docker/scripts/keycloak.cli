embed-server --server-config=standalone-full-ha.xml

/subsystem=keycloak/secure-deployment=tulistop-api.war:add(realm=${env.KEYCLOAK_REALM},resource=${env.KEYCLOAK_RESOURCE},auth-server-url=${env.KEYCLOAK_URL},ssl-required=EXTERNAL,enable-cors=true)
/subsystem=keycloak/secure-deployment=tulistop-api.war/credential=secret:add(value=${env.KEYCLOAK_SECRET})
/subsystem=logging/logger=org.jboss.as.ejb3.invocation:add(level=FATAL)